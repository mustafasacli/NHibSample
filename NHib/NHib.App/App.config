<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <clear />
    <add key="nhib-provider" value="NHibernate.Connection.DriverConnectionProvider" />
    <add key="nhib-driver" value="NHibernate.Driver.MySqlDataDriver" />
    <add key="nhib-connection_string" value="Server=127.0.0.1;database=TestServiceDb;Uid=root;Pwd=;" />
    <add key="nhib-dialect" value="NHibernate.Dialect.MySQL5Dialect" />
  </appSettings>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
<system.data>
    <DbProviderFactories>
      <remove invariant="MySql.Data.MySqlClient" />
      <add name="MySQL Data Provider" invariant="MySql.Data.MySqlClient" description=".Net Framework Data Provider for MySQL" type="MySql.Data.MySqlClient.MySqlClientFactory, MySql.Data, Version=6.9.10.0, Culture=neutral, PublicKeyToken=c5687fc88969c44d" />
    </DbProviderFactories>
  </system.data></configuration>
